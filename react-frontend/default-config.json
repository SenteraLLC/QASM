{
    "name": "Multi-Class App Spring 2023",
    "app": "s3",
    "bucket": "sentera-rogues-data",
    "components": [
        {
            "component": "home"
        },
        {
            "component": "multiclassgrid",
            "display_name": "Multi-Class Grid",
            "grid_width": 1,
            "classes": { 
                "plant_type": { 
                    "selector_type": "radio",
                    "class_values": [ 
                        "Normal",
                        "Delay Rogue",
                        "Hybrid Rogue",
                        "Hybrid Low Rogue",
                        "Other"
                    ],
                    "default": "Normal",
                    "class_colors": {
                        "Delay Rogue": "darkorange",
                        "Hybrid Rogue": "white",
                        "Hybrid Low Rogue": "yellow",
                        "Other": "blue"
                    },
                    "class_overlays": true
                }
            },
            "label_savenames": {
                "Staked": "staked_rogues.json",
                "Unstaked": "assumed_rogues.json"
            },
            "label_loadnames": [
                "staked_rogues.json",
                "staked-rogues.json",
                "unstaked_rogues.json",
                "unstaked-rogues.json",
                "assumed_rogues.json",
                "assumed-rogues.json",
                "unfiltered_rogues.json",
                "unfiltered-rogues.json"
            ],
            "autoload_labels_on_dir_select": false,
            "image_layer_folder_names": [
                [
                    "bottom Thumbnails",
                    "nadir Thumbnails",
                    "oblique Thumbnails"
                ],
                [
                    "bottom",
                    "nadir",
                    "oblique"
                ],
                [
                    "bottom Raw Images",
                    "nadir Raw Images",
                    "oblique Raw Images"
                ]
            ]
        },
        {
            "component": "binaryeditor",
            "display_name": "Binary Editor",
            "mode": "s3directory",
            "dilate_keybind": "w"
        }
    ]
}