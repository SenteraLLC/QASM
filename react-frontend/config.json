{
    "name": "Multi-Class App",
    "app": "s3",
    "bucket": "sentera-rogues-data",
    "components": [
        {
            "component": "home"
        },
        {
            "component": "multiclassgrid",
            "display_name": "Multi-Class Grid",
            "grid_width": 1,
            "classes": { 
                "plant_type": { 
                    "selector_type": "radio",
                    "class_values": [ 
                        "Normal",
                        "Delay Rogue",
                        "Hybrid Rogue",
                        "Hybrid Low Rogue",
                        "Other"
                    ],
                    "default": "Normal",
                    "class_colors": {
                        "Delay Rogue": "darkorange",
                        "Hybrid Rogue": "white",
                        "Hybrid Low Rogue": "yellow",
                        "Other": "blue"
                    },
                    "class_overlays": true
                }
            },
            "label_savenames": {
                "Staked": "staked_rogues.json",
                "Unstaked": "assumed_rogues.json"
            },
            "label_loadnames": [
                "staked_rogues.json",
                "unstaked_rogues.json",
                "assumed_rogues.json",
                "unfiltered_rogues.json"
            ]
        },
        {
            "component": "binaryeditor",
            "display_name": "Binary Editor",
            "mode": "s3directory",
            "dilate_keybind": "w"
        }
    ]
}