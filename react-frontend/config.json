{
    "name": "QASM App",
    "app": "s3",
    "bucket": "stand-qa-data",
    "components": [
        {
            "component": "home"
        },
        {
            "component": "grid",
            "grid_width": 2,
            "classes": [
                {
                    "class_name": "normal", 
                    "svg_overlay": null
                }, 
                {
                    "class_name": "rogue", 
                    "svg_overlay": "x_overlay"
                }
            ]
        },
        {
            "component": "multiclassgrid",
            "display_name": "Multi-Class Grid",
            "grid_width": 2,
            "classes": { 
                "plant_type": { 
                    "selector_type": "radio",
                    "class_values": [ 
                        "Normal",
                        "Delay Rogue",
                        "Hybrid Rogue",
                        "Mix Rogue"
                    ],
                    "default": "Normal"
                },
                "week": { 
                    "selector_type": "checkbox", 
                    "class_values": [ 
                        "Week 1",
                        "Week 2",
                        "Week 3",
                        "Week 4",
                        "Week 5"
                    ]
                },
                "stake_color": { 
                    "selector_type": "checkbox",
                    "class_values": [ 
                        "Yellow",
                        "White",
                        "Red"
                    ]

                } 
            }
        },
        {
            "component": "binaryeditor",
            "display_name": "Binary Editor",
            "mode": "s3directory",
            "dilate_keybind": "w"
        },
        {
            "component": "imagelabeler",
            "display_name": "Image Labeler",
            "image_dir": "mfstand/2022/1051-Thonbontle/AckermannJWSP1A/100722T140321/RGB/",
            "anno_dir": "mfstand/2022/1051-Thonbontle/AckermannJWSP1A/100722T140321/row_annotations/",
            "subtasks": {
                "row_classification": {
                    "display_name": "Row Classification",
                    "classes": [
                        {
                            "name": "Male",
                            "color": "blue",
                            "id": 10
                        },
                        {
                            "name": "Female",
                            "color": "white",
                            "id": 11
                        }
                    ],
                    "allowed_modes": ["polyline"],
                    "resume_from": "row_annotations"
                }
            }
        }
    ]
}